{
  "name": "@snailycad/website",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "format:quick": "pretty-quick --staged --pattern --ignore-path .gitignore \"./**/*.{js,jsx,ts,tsx,md,css,json}\"",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^1.0.0-next.39",
    "@sveltejs/kit": "1.0.0-next.520",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.2.0",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.8"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "svelte-lite-youtube-embed": "^1.1.0",
    "svelte-seo": "^1.4.1",
    "swiper": "^8.4.4"
  }
}
